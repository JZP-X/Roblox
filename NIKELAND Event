-- LoadScript:

local LogoSound = Instance.new("Sound")
LogoSound.SoundId = "rbxassetid://1264316003"
LogoSound.Parent = LoadFrame

LoadFrame.Visible = true

wait(0.1)
LoadIcon.Visible = true

for i = 0, 100, 1 do
	wait(0.01)
	LoadIcon.Rotation = LoadIcon.Rotation + 5
	if LoadIcon.Rotation == 360 then
		LoadIcon.Rotation = 0
	end
end

LoadIcon.Visible = false
LogoIcon.Visible = true
LogoSound:Play()

for i = 0, 10, 1 do
	wait(0.1)
	LogoIcon.ImageTransparency += -0.1
end

wait(4)
for i = 0, 10, 1 do
	wait(0.1)
	LogoIcon.ImageTransparency += 0.1
end

LogoIcon.Visible = false
Label.Visible = true

for i = 0, 10, 1 do
	wait(0.1)
	Label.BackgroundTransparency += -0.1
	Label.TextTransparency += -0.1
end

wait(2)
LoadFrame.Visible = false
MainFrame.Visible = true

-- Scripts:

local Players = game:GetService("Players")
local Player = game.Players.LocalPlayer
local Character = workspace:FindFirstChild(Player.Name)
local RS = game:GetService("ReplicatedStorage")

local State = false
local State_2 = false
local CFolder = workspace.BadgeColliders
local CFolder2 = workspace.PointsOfInterests
local HRemote = workspace

Button.MouseButton1Click:Connect(function()
	if State == false then
		State = true
		Button.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
		if State == true and Character then
			for _,v in ipairs(CFolder:GetChildren()) do
				if v and Character then
					Character.PrimaryPart.CFrame = v.CFrame
					wait(0.5)
				end
			end
		end
	else
		Button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
		wait(0.5)
		State = false
	end
end)

Button_2.MouseButton1Click:Connect(function()
	if State_2 == false then
		State_2 = true
		Button_2.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
		if State_2 == true and Character then
			for _,v in ipairs(CFolder2:GetChildren()) do
				if v:IsA("Part") and Character then
					Character.PrimaryPart.CFrame = v.CFrame
					wait(0.5)
				end
			end
		end
		--while true do
		--	if State_2 == true then
		--		HRemote:InvokeServer(1)
		--		wait(0.01)
		--	else
		--		break
		--	end
		--end
	else
		Button_2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
		wait(0.5)
		State_2 = false
	end
end)

Button_3.MouseButton1Click:Connect(function()
	if Character then
		local Point = CFolder2.PointOfInterest1
		Character.PrimaryPart.CFrame = Point.InteractionPoint.CFrame
	end
end)

Button_4.MouseButton1Click:Connect(function()
	if Character then
		local Point = CFolder2.PointOfInterest2
		Character.PrimaryPart.CFrame = Point.InteractionPoint.CFrame
	end
end)

Button_5.MouseButton1Click:Connect(function()
	if Character then
		local Point = CFolder2.PointOfInterest3
		Character.PrimaryPart.CFrame = Point.InteractionPoint.CFrame
	end
end)

Button_6.MouseButton1Click:Connect(function()
	if Character then
		local Point = CFolder2.PointOfInterest4
		Character.PrimaryPart.CFrame = Point.InteractionPoint.CFrame
	end
end)

Button_7.MouseButton1Click:Connect(function()
	if Character then
		local Point = CFolder2.PointOfInterest5
		Character.PrimaryPart.CFrame = Point.InteractionPoint.CFrame
	end
end)
